<div class="page-header">
    <h1>New User for Account <%- link_to(account.name, path_to.account(account)) %></h1>
</div>

<% form_for(user, {action: path_to.account_users(params.account_id), method: 'POST', id: "user_form", class: 'form-horizontal'}, function (form) { %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.account_users(params.account_id), {class: 'btn'}) %>
    </div>
    <%- partial('users/form.ejs', {locals: {form: form, user: user}}) %>
<% });%>
