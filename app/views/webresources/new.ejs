<div class="page-header">
    <h1>New part for project <%- link_to(project.name, path_to.account_project(params.account_id, project)) %></h1>
</div>
<% form_for(webresource, {action: path_to.account_project_webresources(params.account_id, params.project_id), method: 'POST', id: "webresource_form", class: 'form-horizontal', enctype: 'multipart/form-data'}, function (form) { %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.account_project_webresources(params.account_id, params.project_id), {class: 'btn'}) %>
    </div>
    <%- partial('webresources/form.ejs', {locals: {form: form, webresource: webresource}}) %>
     <div class="control-group">
	   <label for="Robot_thumbnail" class="control-label">File</label>
	   <div class="controls">
	        <input type="file" name="Webresource[resourceFile]">
	   </div>
	</div>
<% });%>
