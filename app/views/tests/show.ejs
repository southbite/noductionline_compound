<ul class="breadcrumb">
    <li><a href="/">Home</a> <span class="divider">/</span></li>
    <li><a href="<%- pathTo.tests() %>">tests</a> <span class="divider">/</span></li>
    <li class="active"><a href="#">Details of test</a></li>
</ul>

<div class="page-header">
    <h1>Details of test</h1>
</div>

<table class="table table-bordered">
    <tbody>
        <tr><th>test</th><td><%= test.test %></td></tr>
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', pathTo.edit_test(test), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', pathTo.test(test), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', pathTo.tests()) %>
</div>
