<% form_for(project, {action: path_to.account_project(params.account_id, project), method: 'PUT', id: "project_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('projects/form.ejs', {locals: {form: form, project: project}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Update project', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.account_project(params.account_id, project), {class: 'btn'}) %>
    </div>
<% });%>