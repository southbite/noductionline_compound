<div class="page-header">
    <h1>Add new project </h1>
</div>

<% form_for(project, {action: path_to.account_projects(params.account_id), method: 'POST', id: "project_form", class: 'form-horizontal'}, function (form) { %>
	 <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create project', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.account_projects(params.account_id), {class: 'btn'}) %>
    </div>
    <%- partial('projects/form.ejs', {locals: {form: form, project: project}}) %>
<% });%>
