<div class="page-header">
    <h1>New robot</h1>
</div>
<% form_for(robot, {action: path_to.account_project_robots(params.account_id, params.project_id), method: 'POST', id: "robot_form", class: 'form-horizontal'}, function (form) { %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.account_project_robots(params.account_id, params.project_id), {class: 'btn'}) %>
    </div>
    <%- partial('robots/formnew.ejs', {locals: {form: form, robot: robot}}) %>
<% });%>
